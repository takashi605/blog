{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "pnpm": "9.9.0",
    "npm": "please_use_pnpm_instead"
  },
  "packageManager": "pnpm@9.9.0",
  "scripts": {
    "web": "pnpm -F web",
    "e2e": "pnpm -F e2e",
    "entities": "pnpm -F entities",
    "blog-admin": "pnpm -F blog-admin",
    "service": "pnpm -F service",
    "shared-interface-adapter": "pnpm -F shared-interface-adapter",
    "shared-test-data": "pnpm -F shared-test-data",
    "e2e-test": "pnpm tsc && pnpm e2e e2e-test",
    "e2e-ui": "pnpm tsc && pnpm e2e e2e-ui",
    "web-build": "pnpm -F web exec next build",
    "web-dev": "pnpm -F web exec next dev",
    "admin-dev": "pnpm -F blog-admin exec next dev",
    "tsc": "pnpm shared-test-data tsc",
    "generate-types": "openapi-typescript ./openapi.json -o ./packages/shared-interface-adapter/src/generated/api-types.ts"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.9",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "openapi-typescript": "^7.4.2",
    "prettier": "^3.4.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.4"
  },
  "dependencies": {
    "jest-fixed-jsdom": "^0.0.9"
  }
}
